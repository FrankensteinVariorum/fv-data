         <anchor type="semantic" subtype="start" xml:id="fMS_box_c57_the_same"/>
         <seg xml:id="C22_app784-fMS">
            <milestone spanTo="#c57-0062.04" unit="tei:head"/>
            <lb n="c57-0062__main__17"/>The same <lb n="c57-0062__main__18"/>chapter <lb n="c57-0062__main__19"/>continued <anchor xml:id="c57-0062.03"/>
            <anchor xml:id="c57-0062.04"/>
            <lb n="c57-0063__main__1"/>
            <del rend="strikethrough" xml:id="c57-0063__main__d4e12348">
